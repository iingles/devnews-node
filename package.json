{
  "name": "devnews-node",
  "version": "1.0.0",
  "description": "experiment with graphql",
  "main": "index.js",
  "scripts": {
    "createDB": "npx prisma2 migrate save --name 'init' --experimental && npx prisma2 migrate up --experimental",
    "generate": "npx prisma2 generate",
    "postinstall": "npm -s run generate",
    "seed": "node -r esm prisma/seed",
    "dev": "npx prisma2 studio --experimental",
    "start": "node -r esm ./src/index"
  },
  "author": "Isaac Ingles",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.0.0-preview023",
    "apollo-server": "^2.11.0",
    "esm": "^3.2.25",
    "graphql": "^14.6.0",
    "nexus": "^0.12.0-rc.13",
    "nexus-prisma": "^0.10.0"    
  },
  "devDependencies": {
    "prisma2": "^2.0.0-preview023"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
